{
  "name": "@geoffcox/sterling-svelte",
  "version": "2.0.7",
  "author": "Geoff Cox",
  "description": "A modern, accessible, and lightweight component library for Svelte.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeoffCox/sterling-svelte/issues"
  },
  "homepage": "https://github.com/GeoffCox/sterling-svelte/blob/main/README.md",
  "keywords": [
    "svelte",
    "svelte5",
    "svelte 5",
    "typescript",
    "javascript",
    "UI",
    "UX",
    "component",
    "components",
    "controls",
    "autocomplete",
    "button",
    "callout",
    "checkbox",
    "dialog",
    "dropdown",
    "input",
    "label",
    "link",
    "list",
    "menu",
    "pagination",
    "popover",
    "progress",
    "radio",
    "select",
    "slider",
    "switch",
    "tab",
    "tabs",
    "tab list",
    "textarea",
    "tooltip",
    "tree"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "build": "vite build && npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "package": "svelte-kit sync && svelte-package && publint",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@fontsource/atkinson-hyperlegible": "^5.1.1",
    "@fontsource/source-code-pro": "^4.5.14",
    "@geoffcox/sterling-svelte-themes": "^2.0.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.4",
    "@types/lodash-es": "^4.17.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.1.0",
    "keyborg": "^2.0.0",
    "lodash-es": "^4.17.21"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
